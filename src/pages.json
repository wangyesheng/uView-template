{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/me/index",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/route/index",
      "style": {
        "navigationBarTitleText": "路线查询"
      }
    },
    {
      "path": "pages/me/setting",
      "style": {
        "navigationBarTitleText": "设置"
      }
    },
    {
      "path": "pages/opinion/index",
      "style": {
        "navigationBarTitleText": "意见反馈"
      }
    },
    {
      "path": "pages/opinion/me",
      "style": {
        "navigationBarTitleText": "我的反馈"
      }
    },
    {
      "path": "pages/order/index",
      "style": {
        "navigationBarTitleText": "历史预约"
      }
    },
    {
      "path": "pages/order/edition",
      "style": {
        "navigationBarTitleText": "编辑预约信息"
      }
    },
    {
      "path": "pages/me/info",
      "style": {
        "navigationBarTitleText": "历史预约",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/data/hotel",
      "style": {
        "navigationBarTitleText": "选择乘车站"
      }
    },
    {
      "path": "pages/data/store",
      "style": {
        "navigationBarTitleText": "选择免税店"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uView UI",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#909399",
    "selectedColor": "#303133",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "black",
    "list": [
      {
        "iconPath": "static/images/tabbar/home.png",
        "selectedIconPath": "static/images/tabbar/home-selected.png",
        "text": "首页",
        "pagePath": "pages/home/index"
      },
      {
        "iconPath": "static/images/tabbar/route.png",
        "selectedIconPath": "static/images/tabbar/route-selected.png",
        "text": "路线查询",
        "pagePath": "pages/route/index"
      },
      {
        "iconPath": "static/images/tabbar/me.png",
        "selectedIconPath": "static/images/tabbar/me-selected.png",
        "text": "我的",
        "pagePath": "pages/me/index"
      }
    ]
  }
}
